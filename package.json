    "material-design-icons": "^3.0.1",
    "ngx-mask": "^6.1.2",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "typescript": "^2.9.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
